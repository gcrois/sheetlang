// Range broadcast - single value to multiple cells
A1:A5 = 10

// Verify broadcast
A6 = A1 + A2 + A3 + A4 + A5  // = 50

// Array expansion - array elements to cells
B1:B5 = [1, 2, 3, 4, 5]

// Verify array expansion
B6 = B1 + B2 + B3 + B4 + B5  // = 15

// 1D horizontal range
C1:G1 = 20

// Verify horizontal
C2 = C1 + D1 + E1 + F1 + G1  // = 100

// 2D range broadcast
A8:C10 = 7
// Creates 3x3 grid: A8-C8, A9-C9, A10-C10, all = 7

// Verify 2D
A11 = A8 + B9 + C10  // = 21

// 2D range with array (row-major order)
D8:F9 = [1, 2, 3, 4, 5, 6]
// D8=1, E8=2, F8=3, D9=4, E9=5, F9=6

// Verify 2D array expansion
D10 = D8 + E8 + F8 + D9 + E9 + F9  // = 21

// Step slicing (every 2nd cell)
A13:A21:2 = 13
// A13=13, A15=13, A17=13, A19=13, A21=13

// Fill gaps to test step
A14 = 14
A16 = 16
A18 = 18
A20 = 20

A22 = A13 + A14 + A15 + A16  // = 56

// Horizontal step
B13:F13:2 = 100
// B13=100, D13=100, F13=100 (skip C13, E13)

C13 = 50
E13 = 50

B14 = B13 + C13 + D13  // = 250

// Single cell range (edge case)
H1:H1 = 42  // = 42

// Array too short (should fill with empty)
H3:H6 = [31, 32]
// H3=31, H4=32, H5=empty, H6=empty

H7 = H3 + H4  // = 63
